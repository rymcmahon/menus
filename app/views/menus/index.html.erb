<div class="menu-index-wrap">
  <table class="table table-hover">
    <thead>
      <tr>
        <th colspan="3" class="menu-table-head">Menus</th>
      </tr>
    </thead>
    <tbody>
      <% @menus.each do |menu| %>
        <tr>
          <td><%= menu.name %></td>
          <td><%= link_to "View", menu_path(:id => menu.id) %></td>
          <td><%= link_to "Edit", edit_menu_path(:id => menu.id) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

